[core]
  excludesfile = ~/.gitignore_global
  editor = vim
  autocrlf = false
[alias]
  a = add
  aa = add .
  br = branch
  c = commit
  ca = commit --amend
  cane = commit --amend --no-edit
  ch = checkout
  chb = checkout -b
  cl = clone
  cm = commit -m
  co = checkout
  d = diff
  do = diff origin
  last = log -1 HEAD
  l = log --oneline -n 10
  m = merge
  ms = merge --squash
  p = pull
  ph = push
  phu = "!git push -u origin $(git rev-parse --abbrev-ref HEAD)"
  pht = push --tags
  pr = pull --rebase
  rs = reset
  rsh = reset --hard
  s = status
  sm = submodule
  sh = stash
  t = tag
[include]
  path = ~/.gitconfig.local
